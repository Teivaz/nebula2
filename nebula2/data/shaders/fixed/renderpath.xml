<?xml version="1.0" encoding="utf-8" ?> 

<RenderPath name="dx7_default" featureset="dx9" shaderpath="home:export/shaders/fixed" >
    <Pass name="color" shader="shaders:pass_color.fx">
        <Phase name="opaque_nolight_nofog" shader="shaders:phase_opaque_nolight_nofog.fx" fourcc="colr" sorting="FrontToBack" lights="On">
            <Sequence shader="shaders:default_nolight_nofog.fx" />
            <Sequence shader="shaders:sky.fx" />
        </Phase>
        <Phase name="opaque" shader="shaders:phase_opaque.fx" fourcc="colr" sorting="FrontToBack" lights="On">
            <Sequence shader="shaders:cubeskybox.fx" />
            <Sequence shader="shaders:terrain.fx" />
            <Sequence shader="shaders:default.fx" />
            <Sequence shader="shaders:default_environment.fx" />
            <Sequence shader="shaders:default_lightmapped.fx" />
            <Sequence shader="shaders:ocean.fx" />
        </Phase>
        <Phase name="multipass" shader="shaders:phase_multipass.fx" fourcc="colr" sorting="FrontToBack" lights="On">
            <Sequence shader="shaders:default_layered.fx" />
        </Phase>
        <Phase name="vsopaque" shader="shaders:phase_vsopaque.fx" fourcc="colr" sorting="FrontToBack" lights="On">
            <Sequence shader="shaders:default_skinned.fx" />
            <Sequence shader="shaders:default_blended.fx" />
            <Sequence shader="shaders:leaf.fx" />
            <Sequence shader="shaders:tree.fx" />        
        </Phase>
        <Phase name="alpha" shader="shaders:phase_alpha.fx" fourcc="colr" sorting="BackToFront" lights="On">
            <Sequence shader="shaders:default_alpha.fx" />
            <Sequence shader="shaders:default_alpha2.fx" />
             <Sequence shader="shaders:default_environment_alpha.fx" />
            <Sequence shader="shaders:terraingrass.fx" />
        </Phase>
        <Phase name="vsalpha" shader="shaders:phase_vsalpha.fx" fourcc="colr" sorting="BackToFront" lights="On">
            <Sequence shader="shaders:particle.fx" />        
        </Phase>
    </Pass>
</RenderPath>
