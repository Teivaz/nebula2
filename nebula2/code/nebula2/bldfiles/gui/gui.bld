begintarget ngui
    annotate "The Nebula2 GUI modules"
    settype lib
    setmodules {
        nguiwidget nguiformlayout nguislider2 nguibutton 
        nguilabel nguitextlabel nguitextbutton nguitextentry nguiipaddressentry
        nguicheckbutton nguicheckbuttongroup nguicheckbuttongroup2
        nguitextview nguicmdentry nguitextureview nguiwindow nguiclientwindow
        nguiconsolewindow nguidockwindow nguitexbrowserwindow nguiwatcherwindow
        nguisysteminfowindow nguitooltip nguidirlister nguicategorybrowser
        nguihorislidergroup nguifiledialog nguiskin nguitickerwidget
        nguitabbedlayout nguimessagebox nguiserver nguicanvas nguidiagramcanvas
        nguiadjustdisplaywindow nguicontextmenu nguidragbox
        nguigraphicsbrowserwindow nguisettingsform nguitexttable nguitextwindow
        nguiscenecontrolwindow nguicoloradjust nguicoloradjustwindow
        nguicolorlabel nguicolorpicker nguicolorslidergroup
        nguibar nguifadeouttextlabel nguislide nguislideshow
        nguieventhandler nguimousecursor nguihardpointsbrowserwindow
        nguihardpointslister
    }
    settargetdeps {
        ntoollib
    }
endtarget

