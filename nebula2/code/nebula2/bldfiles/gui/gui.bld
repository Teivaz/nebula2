begintarget ngui
    annotate "The Nebula2 GUI modules"
    settype lib
    setmodules {
        nguiwidget nguiformlayout nguislider nguislider2 nguibutton nguilabel
        nguitextlabel nguicyclebutton nguitextbutton nguitextentry
        nguiipaddressentry nguicheckbutton nguiflipbutton
        nguicheckbuttongroup nguilister nguifilelister nguitextview
        nguicmdentry nguitextureview nguiwindow nguiclientwindow
        nguiconsolewindow nguidockwindow nguitexbrowserwindow
        nguiwatcherwindow nguisysteminfowindow nguitooltip
        nguidirlister nguicategorybrowser nguihorislidergroup
        nguifiledialog nguiskin nguiserver
    }
endtarget

