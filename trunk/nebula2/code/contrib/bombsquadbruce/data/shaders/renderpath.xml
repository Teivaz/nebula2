<?xml version="1.0" encoding="utf-8" ?>
<RenderPath name="dx7_default" featureset="dx9" shaderpath="data:shaders">
	<Pass name="color" shader="shaders:pass_color.fx">
		<Phase name="opaque_nolight_nofog" shader="shaders:phase_opaque_nolight_nofog.fx" fourcc="colr"
			sorting="FrontToBack" lights="On">
			<Sequence shader="shaders:skybox.fx" />
		</Phase>
		<Phase name="vsopaque" shader="shaders:phase_vsopaque.fx" fourcc="colr" sorting="FrontToBack"
			lights="On">
			<Sequence shader="shaders:default_skinned.fx" />
		</Phase>
		<Phase name="vsalpha" shader="shaders:phase_vsalpha.fx" fourcc="colr" sorting="BackToFront"
			lights="On">
			<Sequence shader="shaders:terrain.fx" />
			<Sequence shader="shaders:default.fx" />
			<Sequence shader="shaders:water.fx" />
			<Sequence shader="shaders:underwater.fx" />
			<Sequence shader="shaders:tree.fx" />
		</Phase>
		<Phase name="alpha" shader="shaders:phase_alpha.fx" fourcc="colr" sorting="BackToFront"
			lights="On">
			<Sequence shader="shaders:shape.fx" />
			<Sequence shader="shaders:gui.fx" />
		</Phase>
	</Pass>
</RenderPath>
